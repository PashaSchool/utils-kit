{
  "name": "export-csv-core",
  "version": "0.0.1",
  "description": "Vanilla JS CSV export library with worker support",
  "author": "Pavlo Kuzina",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts",
      "development": "./src/index.ts"
    },
    "./worker": {
      "import": "./dist/worker.js",
      "types": "./dist/worker.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/wicg-file-system-access": "^2023.10.7",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
